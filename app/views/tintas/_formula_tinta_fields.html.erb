<div>
    <h3>Formulario Anidado</h3>
  <div class="field">
      
       <%=
         f.association :base_tinta,
         collection: BaseTinta.all, 
         label_method: :descripcion_base, 
         value_method: :id, 
         as: :select, 
         include_blank: 'Seleccione Base'
       %>
       
     </div>
     
     <!--  -->
     <div class="field s12">
         
       <%=
       f.input :cantidad
       %>
       
     </div>
     
     <div class="field s12">
         
       <%=
       f.input :estado
       %>
       
     </div>
     
     <%= f.hidden_field :_destroy %>
	<%= link_to "Eliminar", '#', class: "remove_fields" %>


</div>
