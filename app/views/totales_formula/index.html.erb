<p id="notice"><%= notice %></p>

<h1>Totales Formula</h1>

<table>
  <thead>
    <tr>
      <th>Formula tinta</th>
      <th>Total</th>
      <th>Estado</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @totales_formula.each do |total_formula| %>
      <tr>
        <td><%= total_formula.formula_tinta %></td>
        <td><%= total_formula.total %></td>
        <td><%= total_formula.estado %></td>
        <td><%= link_to 'Show', total_formula %></td>
        <td><%= link_to 'Edit', edit_total_formula_path(total_formula) %></td>
        <td><%= link_to 'Destroy', total_formula, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
<!--Boton Crear -->
  <div class="fixed-action-btn">
    <a href="totales_formula/new"  class="btn-floating btn-large red">
      <i class="large material-icons">add_to_photos</i>
    </a>
  </div>
<!--/Boton Crear -->
